using System;

    class Program
    {
        static void Main()
        {
			int [] mt = new int[15];
			Random rnd = new Random();
			for (int i = 0; i < 15; i++)
            {
				mt[i] = rnd.Next(-10, 30);
			}
			int x=0;
			int y=0;
			for (int i = 0; i < 15; i++)
            {
				if(mt[i]>0)
				{
					x++;
					y=y+mt[i];
				}
			}
			Console.WriteLine(y/x);
		}
}
