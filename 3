using System;
					
public class Program
{
	public static void Main()
	{
		int x;
		int [] y = new int[10];
		for(int i=0;i<10;i++)
		{
			x = Convert.ToInt32(Console.ReadLine());
			y[i] = x;
		}
		int c=0;
		for(int i=0;i<10;i++)
		{
			if (i!=9)
			{
			if(y[i]<y[i+1])
			{
				c=1;
			}
			}
		}
		if(c==0)
		{
			Console.WriteLine("В порядке убывания");
		}
		else
		{
			Console.WriteLine("Не в порядке убывания");
		}
	}
}
